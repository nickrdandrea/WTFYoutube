{

    "manifest_version": 2,
    "name": "WTFYoutube",
    "version": "1.0",
  
    "description": "Remove everything but results from Youtube search.",
  
    "icons": {
      "48": "icons/wtfy-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["wtfyoutube.js"]
      }
    ]
  
  }
  