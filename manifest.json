{

    "manifest_version": 2,
    "name": "WTFYoutube",
    "version": "1.0",
  
    "description": "Remove unwanted sections of Youtube search.",
  
    "icons": {
      "48": "icons/wtfy-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["wtfyoutube.js"]
      }
    ],

    "browser_specific_settings": {
      "gecko": {
        "id": "wtfyoutube@nickd.com"
      }
    }
  }
  